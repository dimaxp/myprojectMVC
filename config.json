{
  "db" : {
    "server":"localhost",
    "dbname":"wspace",
    "user":"root",
    "pwd":"12345"
  },
  "urls":{
    "show article":{
      "path":"/",
      "controller":"DIMA\\WSPACE\\Controllers\\IndexController::indexAction",
      "method": "GET"
    },

    "panel":{
      "path":"/panel",
      "controller":"DIMA\\WSPACE\\Controllers\\PanelController::showPanelAction",
      "method": "GET"
    },



    "add_cell":{
      "path":"/panel/addzadacha",
      "controller":"DIMA\\WSPACE\\Controllers\\PanelController::zadachaDobavitAction",
      "method": "POST"
    },
    "registration":{
      "path":"/account/registration",
      "controller":"DIMA\\WSPACE\\Controllers\\AccountController::registrationAction",
      "method": "POST"
    },

    "acc_auth":{
      "path":"/account/auth",
      "controller":"DIMA\\WSPACE\\Controllers\\AccountController::authAction",
      "method": "POST"
    },

    "gotovoZadacha":{
      "path":"/zadacha/gotovo/{id:\\d+}",
      "controller":"DIMA\\WSPACE\\Controllers\\PanelController::zadachaGotovaAction",
      "method": "GET"
    },



    "dobavitZametka":{
      "path":"/panel/addzametka",
      "controller":"DIMA\\WSPACE\\Controllers\\PanelController::zametkaDobavitAction",
      "method": "POST"
    },


    "delZametka":{
      "path":"/zametka/del/{id:\\d+}",
      "controller":"DIMA\\WSPACE\\Controllers\\PanelController::zametkaDelAction",
      "method": "GET"
    },

    "dobavitFile":{
      "path":"/load/files",
      "controller":"DIMA\\WSPACE\\Controllers\\PanelController::fileDobavitAction",
      "method": "POST"
    }







  }
}